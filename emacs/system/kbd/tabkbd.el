;;move to the previous group
(global-set-key  (kbd "C-. k") 'tabbar-backward-group)

;; move to next group
(global-set-key (kbd "C-. i") 'tabbar-forward-group)

;; move to the left tab
(global-set-key (kbd "C-. j") 'tabbar-backward)

;; move to the right tab
(global-set-key (kbd "C-. l") 'tabbar-forward)
